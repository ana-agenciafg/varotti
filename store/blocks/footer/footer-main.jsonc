  },
    "rich-text#footer-title-transporte": {
      "props": {
        "text": "#### Transporte",
        "blockClass": "footer-title"
      }
    },
    "image#footer-transporte-1": {
      "title": "Selo Jamef",
      "props": {
        "src": "/arquivos/transportadora--jamef.png" 
