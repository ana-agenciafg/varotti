    "image#footer-certificados-3": {
      "title": "Selo 3",
      "props": {
        "src": "/arquivos/selo-pci-certified.png" 
      }
    },

  


  
  "flex-layout.col#footer-transporte": {
    "children": [      
      "rich-text#footer-title-transporte",
      "flex-layout.row#footer-transporte"
    ],
    "props": {
      "verticalAlign": "top"
    }
  },
