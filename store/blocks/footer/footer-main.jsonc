      "rich-text#footer-title-pagamento",
      "flex-layout.row#footer-pagamento"
    ],
    "props": {
      "verticalAlign": "top"
    }
  },
  "flex-layout.row#footer-pagamento": {
    "children": [
      "image#footer-pagamento-1",
      "image#footer-pagamento-2",
      "image#footer-pagamento-3",
      "image#footer-pagamento-4",
      "image#footer-pagamento-5",
