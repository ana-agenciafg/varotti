      "preventVerticalStretch": true,
      "horizontalAlign": "left",
      "colGap": 5, 
      "marginTop": 3,
      "preserveLayoutOnMobile": true
    }
  },
    "rich-text#footer-title-certificados": {
      "props": {
        "text": "#### Certificados",
        "blockClass": "footer-title"
      }
    },
    "image#footer-certificados-1": {
      "title": "Selo 1",
      "props": {
        "src": "/arquivos/certificado--lets_encrypt.png" 
      }
    },
    "image#footer-certificados-2": {
      "title": "Selo 2",
